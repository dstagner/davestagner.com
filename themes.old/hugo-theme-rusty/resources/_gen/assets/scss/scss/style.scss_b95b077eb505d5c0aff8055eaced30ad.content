body{background:#4b4b55;margin:0;box-sizing:border-box;word-wrap:break-word;overflow-wrap:break-word}ul{list-style-type:none;margin-block-start:0;margin-block-end:0;padding-inline-start:0}h1,h2,h3,h4,h5{font-family:nunito,Arial,Helvetica,sans-serif;margin:0}p,ul,ol{font-family:merriweather,Georgia,times new roman,serif;margin:0}h1{color:#965d62;font-size:2.5rem}header{background:#2b222c;padding:2rem}header p{color:#f6e7f7;font-size:1.25rem;line-height:55px}header .tag{color:#965d62}.container{width:85%;margin:0 auto}#content{background:#f6e7f7;padding:2rem}#content p{color:#2b222c;margin-bottom:1rem;line-height:28px}#content .type{padding-bottom:2rem}#content .date{color:#4b4b55;font-family:nunito,Arial,Helvetica,sans-serif;font-weight:300;text-transform:uppercase}#content .read-more{margin:0}#content .read-more a:link,#content .read-more a:visited{color:#965d62;font-family:nunito,Arial,Helvetica,sans-serif;text-decoration:none}#content .read-more a:hover{color:#4b4b55}#content h2{font-size:2rem;margin-bottom:1rem}#content h3{font-family:merriweather,Georgia,times new roman,serif;font-size:1.5rem;margin-bottom:1rem}#content h3 a:link,#content h3 a:visited{color:#965d62;text-decoration:none}#content h3 a:hover{color:#2b222c}#content h4{color:#4b4b55;font-family:nunito,Arial,Helvetica,sans-serif;font-size:1.25rem;text-transform:uppercase}#content h5{font-size:1.5rem;margin-bottom:2rem}#content h5 a:link,#content h5 a:visited{color:#4b4b55;text-decoration:none}#content h5 a:hover{color:#965d62}#content pre{background:#4b4b55;border-radius:5px;color:#f6e7f7;padding:1rem;white-space:pre-wrap}#content a:link,#content a:visited{color:#965d62}#content a:hover{color:#4b4b55}#content ul,#content ol{list-style-type:disc;margin:0 0 1rem 1rem}#content ul li,#content ol li{color:#2b222c;margin-bottom:.5rem}#content ol{margin-bottom:.5rem}#content img{width:100%;max-width:500px;margin:0 auto}footer{padding:2rem}footer p{font-family:merriweather,Georgia,times new roman,serif;color:#f6e7f7;margin-bottom:1rem;text-align:center}footer .links{display:flex;flex-flow:row nowrap;justify-content:center}footer .links a:link,footer .links a:visited{border-bottom:2px solid #965d62;color:#965d62;text-decoration:none;margin:0 5px}footer .links a:hover{color:#f6e7f7;border-bottom:2px solid #f6e7f7}.error{display:flex;flex-direction:column;align-items:center;align-content:center;margin-top:15%}@media screen and (min-width:800px){h1{font-size:3rem}header p{font-size:2rem}.container{width:50%;margin:0 auto}}